    "strict": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "jsx": "react-jsx",
    "target": "ESNext",
    "lib": ["ES2023", "DOM"],
    "module": "ESNext",
    "isolatedModules": true,
    "noEmit": true,
    "types": ["kakao.maps.d.ts"]
